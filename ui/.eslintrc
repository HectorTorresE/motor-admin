{
  "extends": [
    "standard",
    "plugin:vue/vue3-recommended"
  ],
  "rules": {
    "vue/no-mutating-props": 0,
    "vue/no-v-html": 0,
    "vue/multi-word-component-names": 0,
    "vue/no-reserved-component-names": 0
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "parser": "@babel/eslint-parser"
  }
}
